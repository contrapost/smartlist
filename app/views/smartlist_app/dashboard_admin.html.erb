<head>
  <% content_for :title, 'DASHBOARD ADMIN' %>
</head>

<body>

<div class="top-content">

  <div class="inner-bg">


    <div class="container">
      <div class="row">
        <div class="col-sm-8 col-sm-offset-2 text">
          <h1 class="page-header"><strong>SMARTLIST ADMIN</strong></h1>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-sm-6">
          <div class="form-box">
            <%= link_to image_tag('users.png', width: 200, height: 200, border: 0), {action: 'show_users', controller: 'admin'}, {class: 'dock-item'} %>
            <p class="dashboard_category_title">ADMIN USERS</p>
            <p class="nospace">
              Create, edit or delete users.
            </p>
          </div>
        </div>
        <div class="col-xs-12 col-sm-6">
          <div class="form-box">
            <%= link_to image_tag('shop_admin.png', width: 200, height: 200, border: 0), {action: 'show_stores', controller: 'admin'}, {class: 'dock-item'} %>
            <p class="dashboard_category_title">ADMIN STORES</p>
            <p class="nospace">
              Create, edit or delete stores.
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</body>
